{
  "scope": "juicyfx",
  "name": "heatbadger",
  "rewrites": [
    { "source": "/favicon.ico", "destination": "https://juicy.vercel.app/favicon.ico" },
    { "source": "/github/readme/([a-z]+)/([a-z\\-]+)/(.*)", "destination": "/api/github/readme.ts?r=$1/$2" }
  ],
  "redirects": [
    { "source": "/", "destination": "https://github.com/juicyfx/juicy/tree/master/tools/heatbadger" }
  ],
  "env": {
    "GITHUB_TOKEN": "@github-juicyfx-heatbadger"
  }
}
