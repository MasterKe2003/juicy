      "dest": "/api/index?vendor=twemoji&icon=$icon&size=$size"
    },
    {
      "src": "/emojitwo/(?<icon>[\\w\\-]+)(:?/(?<size>\\d{2,3})/?)?",
      "dest": "/api/index?vendor=emojitwo&icon=$icon&size=$size"
