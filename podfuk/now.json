{
    "version": 2,
    "scope": "juicyfx",
    "name": "podfuk",
    "alias": ["podfuk.jfx.cz"],
    "builds": [
        { "src": "src/*.ts", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/url/(.*)", "dest": "/src/url.ts" },
        { "src": "/raw/(.*)", "dest": "/src/raw.ts" },
        { "src": "/post/(.*)", "dest": "/src/post.ts" }
    ]
}
