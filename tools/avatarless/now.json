{
  "scope": "juicyfx",
  "name": "avatarless",
  "rewrites": [
    { "source": "/g/(.+@.+\\..+)", "destination": "/api/gravatar.ts?_email=$1" },
    { "source": "/i/(.+)", "destination": "/api/initials.ts?_initials=$1" },
    { "source": "/gi/(.+@.+\\..+)", "destination": "/api/gravatar+initials.ts?_email=$1" },
    { "source": "/(.*)", "destination": "/api/index.ts" }
  ]
}
